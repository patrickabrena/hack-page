pipeline {
    agent any
    stages {
        stage("Pull Repository") {
            steps {
             script {
                def sshHost = "ip-172-31-14-203"
                def sshUser = "ubuntu"
                def privateKeyPath = "/home/ubuntu/.ssh/id_rsa"
                def repoUrl = "git@github.com:patrickabrena/hack-page.git"
                def localPath = "var/www/html"

                sshagent(['2f338a04-1f29-42e8-9ff9-03add894c545']) {
                     sh "ssh -i ${privateKeyPath} ${sshUser}@${sshHost} 'git clone ${repoUrl} ${localPath}'"
                }
             }
           }
        }
    }
}
